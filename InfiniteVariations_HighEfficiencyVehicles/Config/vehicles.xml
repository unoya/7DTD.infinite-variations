<config>
	<!-- Minibike -->
	<set xpath="/vehicles/vehicle[@name='vehicleMinibike']/property[@class='fuelTank']/property[@name='capacity']/@value">100</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMinibike']/property[@class='engine']/property[@name='fuelKmPerL']/@value">1</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMinibike']/property[@class='headlight']/property[@name='bright']/@value">1.3</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMinibike']/property[@name='motorTorque']/@value">2200,1100</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMinibike']/property[@name='velocityMax']/@value">10,8</set>
	<!-- Motorcycle -->
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@class='fuelTank']/property[@name='capacity']/@value">300</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@class='engine']/property[@name='fuelKmPerL']/@value">1.5</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@class='headlight']/property[@name='bright']/@value">1.8</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='motorTorque']/@value">2800,1400</set>
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax']/@value">12,10</set>
	<!-- 4x4Truck -->
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@class='fuelTank']/property[@name='capacity']/@value">1000</set>
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@class='engine']/property[@name='fuelKmPerL']/@value">2</set>
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@class='headlight']/property[@name='bright']/@value">1.5</set>
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='steerRate']/@value">190</set>
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='steerCenteringRate']/@value">100</set>
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='motorTorque']/@value">3600,1800</set>
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='velocityMax']/@value">13,12</set>
	<!-- Gyrocopter -->
	<set xpath="/vehicles/vehicle[@name='vehicleGyrocopter']/property[@class='fuelTank']/property[@name='capacity']/@value">1500</set>
	<set xpath="/vehicles/vehicle[@name='vehicleGyrocopter']/property[@class='engine']/property[@name='fuelKmPerL']/@value">3</set>
	<set xpath="/vehicles/vehicle[@name='vehicleGyrocopter']/property[@name='velocityMax']/@value">13,10</set>
</config>